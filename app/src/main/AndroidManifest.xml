<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name="io.phone.build.voipsdkandroid.ApplicationSetup"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VialerVoIPPlatformIntegrationLayer"
        tools:targetApi="31">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.call.CallActivity"
            android:launchMode="singleTask"
            android:exported="true" />
        <activity
            android:name=".ui.call.IncomingCallActivity"
            android:launchMode="singleTask"
            android:exported="true" />
        <meta-data
            android:name="io.phone.build.voipsdkandroid.CALL_ACTIVITY"
            android:value="io.phone.build.voipexample.ui.call.CallActivity" />

        <meta-data
            android:name="io.phone.build.voipsdkandroid.INCOMING_CALL_ACTIVITY"
            android:value="io.phone.build.voipexample.ui.call.IncomingCallActivity" />
    </application>

</manifest>